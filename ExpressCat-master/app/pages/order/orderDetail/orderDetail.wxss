.l-logList .l-li .log-icon{
	display:block;
	width:25rpx;
	height:25rpx;
	background:#ddd;
  border: 1px solid #b1b1b5;
	border-radius:50%;
	margin-left:-13rpx;
}
.l-logList-con{
	width:98%;
	margin:0 0 0 36rpx;
  margin-top:-20rpx;
}
.l-logList .l-li:first-child .log-icon{
    width:35rpx;
    height:35rpx;
    background:#fff url("data:image/png;base64,") no-repeat;
    background-size:100%;
    margin-left:-16rpx;
    border:none;
}

.l-logList .l-li:last-child{
	padding-bottom:0;
    border-left:2rpx solid #fff;
}
.l-logList .l-li:first-child .log-info-txt{
    color:#333333;
}
.l-logList .l-li:first-child .log-F-txt{
   color:#888888;
}
.l-logList .l-li .log-info-txt{
    font-size:26rpx;
    color:#b2b2b2;
}
.l-logList .l-li .log-F-txt{
    font-size:22rpx;
    color:#b2b2b2;
}

